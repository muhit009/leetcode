class Solution {
    public int uniquePaths(int m, int n) {
        int [] a= new int[n];
        Arrays.fill(a,1);
        for(int i=1;i<m;i++){
            for(int j=1;j<n;j++){
                a[j] += a[j-1];
            }
        }
        return  a[n-1];

    }
}
time complexity: O(m*n)
space complexity: O(n)
