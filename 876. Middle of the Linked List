class Solution {
    public ListNode middleNode(ListNode head) {
        int count=0;
        ListNode temp=head;
        while(temp!=null){
            count++;
            temp=temp.next;

        }
        count= (count/2);
        for(int i=0;i<count;i++){
            head=head.next;

        }
        return head;
    }
}
